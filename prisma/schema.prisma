generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Bot {
  id        String  @id
  name      String
  tokenList Token[]
}

model Token {
  id         Int     @id @default(autoincrement())
  Bot        Bot?    @relation(fields: [botId], references: [id])
  telegramId Int?
  botId      String?
  token      String? @db.Text
}
